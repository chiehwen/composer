{
    "default_context": {
        "title": "Hello"
    },
    "static": [
        {
            "url": "/static",
            "path": "static"
        }
    ],
    "routes": [
        {
            "file": "foo.mako",
            "filters": [
                "mako"
            ],
            "url": "foo"
        },
        {
            "context": {
                "title": "Goodbye!"
            },
            "file": "posts/1.md",
            "filters": [
                "markdown",
                {
                    "file": "post.mako",
                    "id": "mako"
                }
            ],
            "url": "post/1"
        }
    ]
}
