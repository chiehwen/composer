{
    "routes": [
        {
            "url": "foo", 
            "context": {
                "title": "Hello"
            }, 
            "filters": [
                "mako"
            ], 
            "file": "foo.mako"
        }, 
        {
            "url": "post/1", 
            "context": {
                "title": "Hello"
            }, 
            "filters": [
                "markdown", 
                "post"
            ], 
            "file": "posts/1.md"
        }
    ], 
    "static": [
        {
            "url": "/static", 
            "file": "static"
        }
    ], 
    "filters": {
        "mako": {
            "class": "composer.filters:Mako", 
            "kwargs": {}
        }, 
        "post": {
            "class": "composer.filters:MakoContainer", 
            "kwargs": {
                "directories": [
                    "."
                ], 
                "template": "post.mako"
            }
        }, 
        "markdown": {
            "class": "composer.filters:Markdown", 
            "kwargs": {}
        }
    }
}
